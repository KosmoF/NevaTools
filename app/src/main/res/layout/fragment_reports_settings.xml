                </CheckBox>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_print_null_summ"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="7dp"
                android:layout_marginTop="7dp"
                android:layout_marginEnd="7dp"
                android:padding="15dp"
                android:background="@drawable/elem_bg"
                android:gravity="center"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cl_print_extra_blocks">

                <TextView
                    android:id="@+id/tv_print_null_summ"
                    android:layout_width="315dp"
                    android:layout_height="match_parent"
                    android:text="@string/reports_settings_print_null_summ"
                    android:textAlignment="textStart"
                    android:singleLine="true"
                    android:theme="@style/TextStyle"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
